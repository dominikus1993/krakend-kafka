{
  "version": 2,
  "timeout": "3000ms",
  "cache_ttl": "300s",
  "host": [
    "http://fake-backend"
  ],
  "extra_config": {
    "github_com/devopsfaith/krakend-gologging": {
      "level": "INFO",
      "prefix": "[KRAKEND]",
      "syslog": true,
      "stdout": true,
      "format": "default"
    },
    "github_com/devopsfaith/krakend-cors": {
      "allow_origins": [
        "*"
      ],
      "allow_methods": [
        "POST",
        "GET"
      ],
      "expose_headers": [
        "Content-Length"
      ],
      "max_age": "12h"
    },
    "port": 8080,
    "name": "kafka-proxy"
  },
  "endpoints": [
    {
      "endpoint": "/health",
      "extra_config": {
        "github.com/devopsfaith/krakend/proxy": {
          "static": {
            "data": {
              "status": "OK2"
            },
            "strategy": "always"
          }
        }
      },
      "backend": [
        {
          "url_pattern": "/",
          "host": [
            "http://fake-backend"
          ],
          "extra_config": {}
        }
      ]
    },
    {
      "endpoint": "/events",
      "method": "POST",
      "backend": [
        {
          "extra_config": {
            "github.com/devopsfaith/krakend-pubsub/publisher": {
              "topic_url": "kafka://test"
            }
          },
          "host": [
            "kafka://"
          ],
          "disable_host_sanitize": true
        }
      ]
    },
    {
      "endpoint": "/messages",
      "method": "POST",
      "backend": [
        {
          "host": ["rabbit://"],
          "extra_config": {
            "github.com/devopsfaith/krakend-pubsub/publisher": {
              "subscription_url": "myexchange"
            }
          },
          "disable_host_sanitize": true
        }
      ]
    }
  ]
}