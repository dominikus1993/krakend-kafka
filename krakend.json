{
    "version": 2,
    "extra_config": {
      "github_com/devopsfaith/krakend-gologging": {
        "level": "ERROR",
        "prefix": "[KRAKEND]",
        "syslog": false,
        "stdout": true,
        "format": "default"
      }
    },
    "timeout": "3000ms",
    "cache_ttl": "300s",
    "output_encoding": "json",
    "name": "kafka-proxy",
    "port": 8080,
    "endpoints": [
      {
        "endpoint": "/events",
        "method": "POST",
        "extra_config": {},
        "output_encoding": "no-op",
        "concurrent_calls": 1,
        "backend": [
          {
            "extra_config": {
              "github.com/devopsfaith/krakend-pubsub/publisher": {
                "topic_url": "mytopic"
              }
            },
            "host": [
              "kafka://"
            ],
            "disable_host_sanitize": true
          }
        ]
      },
      {
        "endpoint": "/ping",
        "extra_config": {
          "github.com/devopsfaith/krakend/proxy": {
            "static": {
              "data": {
                "ping": "OK"
              },
              "strategy": "always"
            }
          }
        },
        "backend": [
          {
            "url_pattern": "/",
            "host": [
              "http://fake-backend"
            ],
            "extra_config": {},
            "encoding": "json"
          }
        ],
        "output_encoding": "json",
        "concurrent_calls": 1,
        "method": "GET"
      }
    ]
  }